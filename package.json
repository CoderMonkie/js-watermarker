  },
  "homepage": "https://codermonkie.github.io/watermarker/",
  "main": "./dist/watermarker.umd.js",
  "module": "./dist/watermarker.es.js",
  "exports": {
    ".": {
      "import": "./dist/watermarker.es.js",
      "require": "./dist/watermarker.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
