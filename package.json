    "publish:major": "npm run build && npm version major && npm publish && git push"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "element-plus": "^1.0.2-beta.53",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "vite": "^2.3.8",
    "vue": "^3.0.5"
  }
